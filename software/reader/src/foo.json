#include <Arduino.h>
#include <SPI.h>
#include <MFRC522.h>

#define SS_PIN 21
#define RST_PIN 26

MFRC522 mfrc522(SS_PIN, RST_PIN);

void setup()
{

  Serial.begin(115200);
  while (!Serial);

  SPI.begin(5, 19, 18, 26);
  mfrc522.PCD_Init();
  mfrc522.PCD_DumpVersionToSerial();

}

void loop() 
{
}
